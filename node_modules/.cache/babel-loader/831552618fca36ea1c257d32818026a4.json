AM,KAAI,GAAI,WAAW,CAAC,YAAM;AAC5B,YAAM,IAAG,GAAI,IAAI,IAAJ,EAAb,CAD4B,CACH;;AACzB,YAAM,OAAM,GAAI,IAAI,IAAJ,CAAS,IAAT,EAAe,CAAf,EAAkB,EAAlB,EAAsB,EAAtB,EAA0B,EAA1B,EAA8B,EAA9B,EAAkC,EAAlC,CAAhB,CAF4B,CAE2B;;AACvD,YAAM,QAAO,GAAI,OAAO,CAAC,OAAR,KAAoB,IAAI,CAAC,OAAL,EAArC,CAH4B,CAGyB;;AAErD,YAAG,QAAO,GAAI,CAAd,EAAiB;AAAE;AACf,UAAA,aAAa,CAAC,KAAD,CAAb;AACA,UAAA,KAAI,CAAC,OAAL,GAAe,IAAf;AACA,UAAA,KAAI,CAAC,MAAL,GAAc,IAAd;AACA;AACJ;;AAEA,YAAM,IAAG,GAAI,IAAI,CAAC,KAAL,CAAY,QAAO,GAAI,KAAI,CAAC,KAA5B,CAAb;AACA,YAAM,KAAI,GAAI,IAAI,CAAC,KAAL,CAAY,QAAO,GAAI,KAAI,CAAC,KAAjB,GAA0B,KAAI,CAAC,MAA1C,CAAd;AACA,YAAM,OAAM,GAAI,IAAI,CAAC,KAAL,CAAY,QAAO,GAAI,KAAI,CAAC,MAAjB,GAA2B,KAAI,CAAC,QAA3C,CAAhB;AACA,YAAM,OAAM,GAAI,IAAI,CAAC,KAAL,CAAY,QAAO,GAAI,KAAI,CAAC,QAAjB,GAA6B,KAAI,CAAC,QAA7C,CAAhB;AAEA,QAAA,KAAI,CAAC,cAAL,GAAsB,KAAI,CAAC,SAAL,CAAe,OAAf,CAAtB;AACA,QAAA,KAAI,CAAC,cAAL,GAAsB,KAAI,CAAC,SAAL,CAAe,OAAf,CAAtB;AACA,QAAA,KAAI,CAAC,YAAL,GAAoB,KAAI,CAAC,SAAL,CAAe,KAAf,CAApB;AACA,QAAA,KAAI,CAAC,WAAL,GAAmB,KAAI,CAAC,SAAL,CAAe,IAAf,CAAnB;AACA,QAAA,KAAI,CAAC,MAAL,GAAc,IAAd;AACH,OAtBwB,EAsBtB,IAtBsB,CAAzB;AAuBJ;AA1BK;AAxBE,CAAf","sourcesContent":["<template>\r\n  <div class=\"date-countdown container-fluid text-center\">\r\n      <div class=\"countdown-content\">\r\n        <div class=\"savedate m-auto\" v-if=\"!expired\">\r\n            <button class=\"savedate-btn py-2 px-5 font-title\">\r\n                Save The Date\r\n            </button>\r\n        </div>\r\n        <div class=\"savedate m-auto\" v-else>\r\n            <button class=\"savedate-btn py-2 px-5 font-title\">\r\n                Save The Date\r\n            </button>\r\n        </div>\r\n        <div class=\"countdown font-title\" v-if=\"loaded\">\r\n            <div class=\"countdown-wrapper row d-flex justify-content-center gap-3\">\r\n                <div class=\"box-date col-lg-1 col-4 mx-lg-5\" id=\"box-hari\">\r\n                    <h2>{{ displayDays }}</h2>\r\n                    <h6>Hari</h6>\r\n                </div>\r\n                <div class=\"box-date col-lg-1 col-4 mx-lg-5\" id=\"box-jam\">\r\n                    <h2>{{ displayHours }}</h2>\r\n                    <h6>Jam</h6>\r\n                </div>\r\n                <div class=\"box-date col-lg-1 col-4 mx-lg-5\" id=\"box-menit\">\r\n                    <h2>{{ displayMinutes }}</h2>\r\n                    <h6>Menit</h6>\r\n                </div>\r\n                <div class=\"box-date col-lg-1 col-4 mx-lg-5\" id=\"box-detik\">\r\n                    <h2>{{ displaySeconds }}</h2>\r\n                    <h6>Detik</h6>\r\n                </div>\r\n            </div>\r\n        </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data: () => ({\r\n        displayDays: 0,\r\n        displayHours: 0,\r\n        displayMinutes: 0,\r\n        displaySeconds: 0,\r\n        loaded: false, // Agar Ketika di Refresh, Countdown Tidak Berubah Jadi 00:00:00:00\r\n        expired: false //Mengatur jika waktu countdown sudah habis\r\n    }),\r\n    computed: {\r\n        _seconds: () => 1000,\r\n        _minutes() {\r\n            return this._seconds * 60\r\n        },\r\n        _hours() {\r\n            return this._minutes * 60\r\n        },\r\n        _days() {\r\n            return this._hours * 24\r\n        },\r\n    },\r\n    mounted() {\r\n        this.showRemaining();\r\n    },\r\n    methods: {\r\n        formatNum: num => (num < 10 ? \"0\" + num : num), // Format Untuk Mendisplay Date\r\n        showRemaining() {\r\n            const timer = setInterval(() => {\r\n                const date = new Date(); // Mengambil Date \r\n                const dateEnd = new Date(2022, 0, 17, 10, 10, 10, 10); //Atur Countdown\r\n                const distance = dateEnd.getTime() - date.getTime(); //Mencari jarak detik antara tanggal yang ditentukan dan tanggal sekarang\r\n\r\n                if(distance < 0) { // Jika waktu countdown sudah habis, clearInterval Timer\r\n                    clearInterval(timer);\r\n                    this.expired = true;\r\n                    this.loaded = true;\r\n                    return\r\n                }\r\n\r\n                const days = Math.floor((distance / this._days));\r\n                const hours = Math.floor((distance % this._days) / this._hours);\r\n                const minutes = Math.floor((distance % this._hours) / this._minutes);\r\n                const seconds = Math.floor((distance % this._minutes) / this._seconds);\r\n\r\n                this.displaySeconds = this.formatNum(seconds);\r\n                this.displayMinutes = this.formatNum(minutes);\r\n                this.displayHours = this.formatNum(hours);\r\n                this.displayDays = this.formatNum(days);\r\n                this.loaded = true;\r\n            }, 1000);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    .date-countdown {\r\n        background: url(\"../assets/images/WoodBG2.jpeg\");\r\n        background-size: cover;\r\n        background-position: center;\r\n        min-height: 50vh;\r\n        position: relative;\r\n    }\r\n\r\n    .date-countdown::after {\r\n        content: '';\r\n        position: absolute;\r\n        bottom: 0;\r\n        left: 0;\r\n        width: 100%;\r\n        height: 100%;\r\n        background: linear-gradient(0deg, #89492bbd 100%, #ae664586);\r\n    }\r\n\r\n\r\n    .savedate-btn {\r\n        background-color: var(--tertiary-color);\r\n        border: none;\r\n        border-radius: 10px;\r\n        outline: none;\r\n        font-size: 1.5rem;\r\n        transform: translateY(-5rem);\r\n    }\r\n    \r\n    .countdown {\r\n        width: 90%;\r\n        position: absolute;\r\n        top: 0;\r\n        left: 50%;\r\n        transform: translateX( -50%);\r\n        z-index: 100;\r\n    }\r\n\r\n    .box-date {\r\n        background: var(--tertiary-color);\r\n        padding: .5rem 3rem;\r\n        border-radius: 11px;\r\n    }\r\n</style>"],"sourceRoot":""}]}