,EAAI,KAAJ,EAAO,CAAA;AAAA;AAAP,C;;;AAEC,EAAA,KAAK,EAAC,6B;AAA8B,EAAA,EAAE,EAAC;;;+BAExC,mBAAA,CAAc,IAAd,EAAc,IAAd,EAAI,OAAJ,EAAS,CAAA;AAAA;AAAT,C;;;AAEC,EAAA,KAAK,EAAC,6B;AAA8B,EAAA,EAAE,EAAC;;;+BAExC,mBAAA,CAAc,IAAd,EAAc,IAAd,EAAI,OAAJ,EAAS,CAAA;AAAA;AAAT,C;;;uBAvBlB,mBAAA,CA4BM,KA5BN,EAAA,UAAA,EA4BM,CA3BF,mBAAA,CA0BM,KA1BN,EAAA,UAAA,EA0BM,CAzBJ,UAyBI,EApBoC,IAAA,CAAA,M,kBAAxC,mBAAA,CAmBM,KAnBN,EAAA,UAAA,EAmBM,CAlBF,mBAAA,CAiBM,KAjBN,EAAA,UAAA,EAiBM,CAhBF,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFF,mBAAA,CAA0B,IAA1B,EAA0B,IAA1B,EAA0B,gBAAA,CAAnB,IAAA,CAAA,WAAmB,CAA1B,EAAkB;AAAA;AAAlB,GAEE,EADF,UACE,CAHN,CAgBE,EAZF,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFF,mBAAA,CAA2B,IAA3B,EAA2B,IAA3B,EAA2B,gBAAA,CAApB,IAAA,CAAA,YAAoB,CAA3B,EAAmB;AAAA;AAAnB,GAEE,EADF,UACE,CAHN,CAYE,EARF,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFF,mBAAA,CAA6B,IAA7B,EAA6B,IAA7B,EAA6B,gBAAA,CAAtB,IAAA,CAAA,cAAsB,CAA7B,EAAqB;AAAA;AAArB,GAEE,EADF,WACE,CAHN,CAQE,EAJF,mBAAA,CAGM,KAHN,EAAA,WAAA,EAGM,CAFF,mBAAA,CAA6B,IAA7B,EAA6B,IAA7B,EAA6B,gBAAA,CAAtB,IAAA,CAAA,cAAsB,CAA7B,EAAqB;AAAA;AAArB,GAEE,EADF,WACE,CAHN,CAIE,CAjBN,CAkBE,CAnBN,C,qCAoBI,CA1BN,CA2BE,CA5BN,C","sourcesContent":["<template>\r\n  <div class=\"date-countdown container-fluid text-center wood-bg\">\r\n      <div class=\"countdown-content wrapper-z-idx\">\r\n        <div class=\"savedate m-auto\">\r\n            <button class=\"savedate-btn py-2 px-5 font-title\">\r\n                Save The Date\r\n            </button>\r\n        </div>\r\n        <div class=\"countdown font-title\" v-if=\"loaded\">\r\n            <div class=\"countdown-wrapper row d-flex justify-content-center\">\r\n                <div class=\"box-date col-lg-2 col-4 m-3\" id=\"box-hari\">\r\n                    <h2>{{ displayDays }}</h2>\r\n                    <h6>Hari</h6>\r\n                </div>\r\n                <div class=\"box-date col-lg-2 col-4 m-3\" id=\"box-jam\">\r\n                    <h2>{{ displayHours }}</h2>\r\n                    <h6>Jam</h6>\r\n                </div>\r\n                <div class=\"box-date col-lg-2 col-4 m-3\" id=\"box-menit\">\r\n                    <h2>{{ displayMinutes }}</h2>\r\n                    <h6>Menit</h6>\r\n                </div>\r\n                <div class=\"box-date col-lg-2 col-4 m-3\" id=\"box-detik\">\r\n                    <h2>{{ displaySeconds }}</h2>\r\n                    <h6>Detik</h6>\r\n                </div>\r\n            </div>\r\n        </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data: () => ({\r\n        displayDays: 0,\r\n        displayHours: 0,\r\n        displayMinutes: 0,\r\n        displaySeconds: 0,\r\n        loaded: false, // Agar Ketika di Refresh, Countdown Tidak Berubah Jadi 00:00:00:00\r\n        expired: false //Mengatur jika waktu countdown sudah habis\r\n    }),\r\n    computed: {\r\n        _seconds: () => 1000,\r\n        _minutes() {\r\n            return this._seconds * 60\r\n        },\r\n        _hours() {\r\n            return this._minutes * 60\r\n        },\r\n        _days() {\r\n            return this._hours * 24\r\n        },\r\n    },\r\n    mounted() {\r\n        this.showRemaining();\r\n    },\r\n    methods: {\r\n        formatNum: num => (num < 10 ? \"0\" + num : num), // Format Untuk Mendisplay Date\r\n        showRemaining() {\r\n            const timer = setInterval(() => {\r\n                const date = new Date(); // Mengambil Date \r\n                const dateEnd = new Date(2022, 0, 12, 10, 10, 10, 10); //Atur Countdown\r\n                const distance = dateEnd.getTime() - date.getTime(); //Mencari jarak detik antara tanggal yang ditentukan dan tanggal sekarang\r\n\r\n                if(distance < 0) { // Jika waktu countdown sudah habis, clearInterval Timer\r\n                    clearInterval(timer);\r\n                    this.expired = true;\r\n                    this.loaded = true;\r\n                    return\r\n                }\r\n\r\n                const days = Math.floor((distance / this._days));\r\n                const hours = Math.floor((distance % this._days) / this._hours);\r\n                const minutes = Math.floor((distance % this._hours) / this._minutes);\r\n                const seconds = Math.floor((distance % this._minutes) / this._seconds);\r\n\r\n                this.displaySeconds = this.formatNum(seconds);\r\n                this.displayMinutes = this.formatNum(minutes);\r\n                this.displayHours = this.formatNum(hours);\r\n                this.displayDays = this.formatNum(days);\r\n                this.loaded = true;\r\n            }, 1000);\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    .date-countdown {\r\n        height: 300px;\r\n    }\r\n\r\n\r\n    .savedate-btn {\r\n        background-color: var(--tertiary-color);\r\n        border: none;\r\n        border-radius: 10px;\r\n        outline: none;\r\n        font-size: 1.5rem;\r\n        transform: translateY(-5rem);\r\n        cursor: pointer;\r\n    }\r\n    \r\n    .countdown-content {\r\n        width: 90%;\r\n    }\r\n\r\n    .countdown {\r\n        margin-top: -2rem;\r\n    }\r\n\r\n    .box-date {\r\n        background: var(--tertiary-color);\r\n        padding: .5rem 3rem;\r\n        border-radius: 11px;\r\n    }\r\n\r\n</style>"],"sourceRoot":""}]}