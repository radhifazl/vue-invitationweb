ne\">\r\n            <div class=\"timeline\">\r\n                <ul class=\"timeline-list\">\r\n                    <li v-for=\"story in stories\" :key=\"story.title\" class=\"story-item mb-5\">\r\n                        <div class=\"tl-content p-2 px-3\">\r\n                            <h3 class=\"font-title\">\r\n                                {{ story.title }}\r\n                            </h3>\r\n\r\n                            <p class=\"desc font-sub\">\r\n                                {{ story.desc }}\r\n                            </p>\r\n                        </div>\r\n\r\n                        <div class=\"tl-date mt-3\">\r\n                            <h3 class=\"font-title\">\r\n                                {{ story.date }}\r\n                            </h3>\r\n                        </div>\r\n                    </li>\r\n                </ul>\r\n            </div>\r\n        </div>\r\n      </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n    data() {\r\n        return {\r\n            stories: [\r\n                {\r\n                title: \"Kenalan\",\r\n                date: \"Mei 2017\",\r\n                desc: \"Kisah ini berawal dari social media, N mengirimkan direct message kepada Q dengan bertanya - tanya perihal kegiatan sekolah\"\r\n                },\r\n                {\r\n                title: \"Melamar\",\r\n                date: \"Oktober 2021\",\r\n                desc: \"Pada tanggal 17 Oktober 2021, N membawa keluarganya bersilaturahmi ke rumah Q dan merencanakan pernikahan bersama, beserta keluarga.\"\r\n                },\r\n                {\r\n                title: \"Akad Nikah\",\r\n                date: \"Januari 2022\",\r\n                desc: \"Setelah bersilaturahmi dan berdiskusi bersama dengan kedua keluarga, hingga diputuskanlah akad nikah pada hari Minggu, tanggal 9 Januari 2022.\"\r\n                },\r\n            ]\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n    .journey-timeline {\r\n        height: 1000px;\r\n        position: relative;\r\n        overflow: hidden;\r\n    }\r\n\r\n\r\n    .journey-timeline-wrapper {\r\n        width: 90%;\r\n    }\r\n\r\n    .journey-timeline-wrapper > #wf-left1, #wf-right1, #wf-right2 {\r\n        position: absolute;\r\n    }\r\n\r\n    #wf-left1 {\r\n        width: 400px;\r\n        top: 0;\r\n        left: -10%;\r\n    }\r\n\r\n    #wf-right1, #wf-right2 {\r\n        right: -10%;\r\n        width: 350px;\r\n    }\r\n\r\n    #wf-right2 {\r\n        bottom: -60%;\r\n    }\r\n\r\n    #wf-right1 {\r\n        top: -10%;\r\n    }\r\n\r\n    /* === Membuat Background Bunga Responsif === */\r\n    @media screen and (max-width: 768px) {\r\n        #wf-left1, #wf-right1, #wf-right2 {\r\n            width: 300px;\r\n        }\r\n\r\n        #wf-left1 {\r\n            top: 20%;\r\n        }\r\n\r\n        #wf-right1 {\r\n            top: -5%;\r\n        }\r\n\r\n        #wf-right2 {\r\n            bottom: -25%;\r\n            right: -20%;\r\n        }\r\n    }\r\n\r\n    .vertical-timeline {\r\n        position: relative;\r\n        height: 100%;\r\n    }\r\n\r\n    .vertical-timeline::after {\r\n        content: '';\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0;\r\n        width: 4px;\r\n        height: 100%;\r\n        background: var(--second-fc);\r\n    }\r\n\r\n    .story-item {\r\n        position: relative;\r\n    }\r\n\r\n    .tl-content:nth-child(odd):after {\r\n        content: '';\r\n        position: absolute;\r\n        top: 10%;\r\n        left: -5%;\r\n        border-top: 20px solid transparent;\r\n        border-bottom: 20px solid transparent;\r\n        border-right: 50px solid #B3541E;\r\n        z-index: -1;\r\n    }\r\n\r\n    .story-item::before {\r\n        content: '';\r\n        position: absolute;\r\n        top: 14%;\r\n        left: -12%;\r\n        background: url(\"../assets/images/lovelist.svg\");\r\n        background-size: cover;\r\n        background-position: center;\r\n        width: 25px;\r\n        height: 25px;\r\n        z-index: 2;\r\n    }\r\n\r\n    .tl-content {\r\n        background: var(--secondary-color);\r\n        border-radius: 20px;\r\n        position: relative;\r\n    }\r\n\r\n    @media screen and (min-width: 1200px) {\r\n        .journey-timeline {\r\n            height: 900px;\r\n        }\r\n\r\n        .vertical-timeline::after {\r\n            left: 50%;\r\n        }\r\n\r\n        .journey-timeline-wrapper {\r\n            width: 85%;\r\n        }\r\n\r\n        .tl-content {\r\n            width: 500px;\r\n        }\r\n\r\n        .story-item {\r\n            display: flex;\r\n            justify-content: space-between;\r\n        }\r\n\r\n        .story-item:nth-child(odd) {\r\n            flex-direction: row-reverse;\r\n        }\r\n\r\n        .story-item::after {\r\n            left: -4%;\r\n        }\r\n\r\n        .story-item:nth-child(even) .tl-content::after {\r\n            content: '';\r\n            position: absolute;\r\n            top: 10%;\r\n            left: 475px;\r\n            border-top: 20px solid transparent;\r\n            border-bottom: 20px solid transparent;\r\n            border-right: none;\r\n            border-left: 50px solid #B3541E;\r\n            z-index: -1;\r\n        }\r\n\r\n        .story-item::before {\r\n            left: 47.5%;\r\n        }\r\n\r\n        .tl-date {\r\n            position: absolute;\r\n        }\r\n\r\n        .story-item:nth-child(odd) .tl-date {\r\n            left: 30%;\r\n        }\r\n\r\n        .story-item:nth-child(even) .tl-date {\r\n            right: 33%;\r\n        }\r\n    }\r\n</style>"],"sourceRoot":""}]}