olute;\r\n      top: 15%;\r\n      padding: 8px 16px;\r\n  }\r\n\r\n  .timeline-list .tl-time h5 {\r\n      color: var(--white-fc);\r\n  }\r\n\r\n  .timeline-list li:nth-child(even) .tl-time {\r\n      right: -100%;\r\n  }\r\n\r\n  .timeline-list li:nth-child(odd) .tl-time {\r\n      left: -40%;\r\n  }\r\n\r\n  @media screen and (max-width: 992px) {\r\n      .timeline::before {\r\n          left: -5%;\r\n      }\r\n\r\n      .tl-content {\r\n          width: 100% !important;\r\n      }\r\n\r\n      .tl-content h3 {\r\n        font-size: 1.2rem;\r\n      }\r\n\r\n      .tl-content::after {\r\n        left: -8%;\r\n        border-right: 50px solid var(--secondary-color);\r\n      }\r\n\r\n\r\n      .timeline-list li {\r\n        width: 100%;\r\n      }\r\n\r\n\r\n      .timeline-list li:nth-child(odd) {\r\n          float: none;\r\n          clear: none;\r\n      }\r\n      \r\n      .timeline-list li:nth-child(even) {\r\n          float: none;\r\n          clear: none;\r\n          transform: translateX(0);\r\n      }\r\n      \r\n      .timeline-list .tl-time {\r\n          position: static;\r\n          padding: 0;\r\n          margin-top: 5rem;\r\n      }\r\n  }\r\n</style>"],"sourceRoot":""}]}