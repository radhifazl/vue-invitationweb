een and (max-width: 768px) {\r\n        #wf-left1, #wf-right1, #wf-right2 {\r\n            width: 300px;\r\n        }\r\n\r\n        #wf-left1 {\r\n            top: 20%;\r\n        }\r\n\r\n        #wf-right1 {\r\n            top: -5%;\r\n        }\r\n\r\n        #wf-right2 {\r\n            bottom: -25%;\r\n            right: -20%;\r\n        }\r\n    }\r\n\r\n    .vertical-timeline {\r\n        position: relative;\r\n        height: 100%;\r\n    }\r\n\r\n    .vertical-timeline::after {\r\n        content: '';\r\n        position: absolute;\r\n        top: 0;\r\n        left: 0;\r\n        width: 4px;\r\n        height: 100%;\r\n        background: var(--second-fc);\r\n    }\r\n\r\n    .story-item {\r\n        position: relative;\r\n    }\r\n\r\n    .tl-content:nth-child(odd):after {\r\n        content: '';\r\n        position: absolute;\r\n        top: 10%;\r\n        left: -5%;\r\n        border-top: 20px solid transparent;\r\n        border-bottom: 20px solid transparent;\r\n        border-right: 50px solid #B3541E;\r\n        z-index: -1;\r\n    }\r\n\r\n    .story-item::before {\r\n        content: '';\r\n        position: absolute;\r\n        top: 14%;\r\n        left: -12%;\r\n        background: url(\"../assets/images/lovelist.svg\");\r\n        background-size: cover;\r\n        background-position: center;\r\n        width: 25px;\r\n        height: 25px;\r\n        z-index: 2;\r\n    }\r\n\r\n    .tl-content {\r\n        background: var(--secondary-color);\r\n        border-radius: 20px;\r\n        position: relative;\r\n    }\r\n\r\n    @media screen and (min-width: 1200px) {\r\n        .journey-timeline {\r\n            height: 900px;\r\n        }\r\n\r\n        .vertical-timeline::after {\r\n            left: 50%;\r\n        }\r\n\r\n        .journey-timeline-wrapper {\r\n            width: 85%;\r\n        }\r\n\r\n        .tl-content {\r\n            width: 500px;\r\n        }\r\n\r\n        .story-item {\r\n            display: flex;\r\n            justify-content: space-between;\r\n        }\r\n\r\n        .story-item:nth-child(odd) {\r\n            flex-direction: row-reverse;\r\n        }\r\n\r\n        .story-item::after {\r\n            left: -4%;\r\n        }\r\n\r\n        .story-item:nth-child(even) .tl-content::after {\r\n            content: '';\r\n            position: absolute;\r\n            top: 10%;\r\n            left: 475px;\r\n            border-top: 20px solid transparent;\r\n            border-bottom: 20px solid transparent;\r\n            border-right: none;\r\n            border-left: 50px solid #B3541E;\r\n            z-index: -1;\r\n        }\r\n\r\n        .story-item::before {\r\n            left: 47.5%;\r\n        }\r\n\r\n        .tl-date {\r\n            position: absolute;\r\n        }\r\n\r\n        .story-item:nth-child(odd) .tl-date {\r\n            left: 30%;\r\n        }\r\n\r\n        .story-item:nth-child(even) .tl-date {\r\n            right: 33%;\r\n        }\r\n    }\r\n</style>"]}]}